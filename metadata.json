{
  "name": "puppet-sudo",
  "version": "0.1.0",
  "author": "Shaheed",
  "summary": "Manage sudo configuration via Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/shaheed121/puppet-sudo",
  "project_page": "https://github.com/shaheed121/puppet-sudo",
  "issues_url": "https://github.com/shaheed121/puppet-sudo/issues",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 1.0.0"}
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": ["6", "7"]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.5.0 < 5.0.0"
    }
  ]
}
